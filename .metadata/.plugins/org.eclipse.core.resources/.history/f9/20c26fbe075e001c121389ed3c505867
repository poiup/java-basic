package game.intercation;

public class Orc {
	private int hp;
	private int atk;
	private int def;
	
	public Orc() {
		hp = 10;
		atk = 2;
		def = 0;
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}

	public int getAtk() {
		return atk;
	}

	public void setAtk(int atk) {
		this.atk = atk;
	}

	public int getDef() {
		return def;
	}

	public void setDef(int def) {
		this.def = def;
	}
}
