package game.polymorphism;

// 모든 사냥감의 뼈대가 되는 클래스
public class Monster {	
	
	// name, hp, mp, atk  
	private String name;
	private int hp;
	private int mp;
	private int atk;
	
	public Monster(String name, int hp, int mp, int atk) {
		this.name = name;
		this.hp =hp;
		this.mp = mp;
		this.atk =atk;
	}
}
